{
  "name": "koa-template",
  "version": "1.0.0",
  "description": "Koa v2 site template.",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmcclare/koa-template"
  },
  "main": "build/index.js",
  "scripts": {
    "start": "NODE_PATH=lib:site_modules NODE_ENV=development nodemon -e coffee,styl,pug,json --ignore ./build --exec 'coffee --transpile ./'",
    "test": "NODE_PATH=lib:site_modules NODE_ENV=test nodemon -e coffee,styl,pug,json --exec 'mocha --check-leaks --reporter nyan dev-test'",
    "build": "gulp",
    "cont-build": "nodemon -e coffee,styl,pug,js,css,json,jpg,jpeg,png,ico --ignore ./build --exec 'gulp'",
    "clean": "gulp clean",
    "start-prod": "NODE_PATH=build/lib:build/site_modules node ./build",
    "test-prod": "NODE_PATH=build/lib:build/site_modules NODE_ENV=test mocha --check-leaks --reporter nyan ./build/test/index.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "coffeescript": "^2.2.2",
    "koa": "^2.5.0",
    "koa-pug": "^3.0.0-2",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "koa-stylus": "^0.1.0",
    "kouto-swiss": "^1.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-coffee": "^3.0.2",
    "gulp-newer": "^1.4.0",
    "gulp-shell": "^0.6.5",
    "gulp-stylus": "^2.7.0",
    "mocha": "^5.0.4",
    "nodemon": "^1.17.1",
    "rsync": "^0.6.1",
    "should": "^13.2.1",
    "supertest": "^3.0.0"
  }
}
